<h1>Sign in</h1>
<%= form_for :user, :url => url_for(:controller => 'sessions', :action => 'create') do |f| %>
  <%= f.label :email %>
  <%= f.text_field :email %><br>
  <%= f.label :password %>
  <%= f.password_field :password %><br><br>
  <%= f.submit "Sign in" %>
<% end %>

<p>or</p>
<div>
  <%= link_to image_tag("https://lh3.googleusercontent.com/JOb-1edXXl9_rrXDIOmDwMj_aTPyzysn6-vSAq_VV1AYV_M_2gec7TDRBd_R-qchK8j8=s170"), '/auth/github', class: "github_button" %>
</div>
